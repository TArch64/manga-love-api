generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  username String @db.VarChar(255) @unique
  email    String @db.VarChar(255) @unique
  password String
}
